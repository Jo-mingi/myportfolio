<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mr.web.mapper.MemoMapper">
    
    <insert id="memoRegister" parameterType="memoVO">
        insert into memo(id, itemName, memo) values(#{id}, #{itemName}, #{memo})
    </insert>
    
    
    
    
    
    
    
</mapper>